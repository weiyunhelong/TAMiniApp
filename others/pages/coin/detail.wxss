/* others/pages/coin/detail.wxss */

/*当前选择*/
.coinchkh {
  height: 100rpx;
  display: flex;
  line-height: 100rpx;
  justify-content: space-between;
  width: 100%;
  color: #909599;
  background-color: #f5f5f5;
}
.coinchkh .leftv{
  margin-left: 20rpx;
  border-bottom: 1rpx solid #f2f2f2;
}
.coinchkh .rightv{
  margin-right: 20rpx;
  border-bottom: 1rpx solid #f2f2f2;
}
/*已选择*/
.coinh {
  height: 100rpx;
  display: flex;
  line-height: 100rpx;
  border-bottom: 1rpx solid #f2f2f2;
  justify-content: space-between;
  margin: 0 20rpx;
  color: #909599;
}
.leftv{
  font-size: 32rpx;
  display: flex;
}

.enname{
  margin-left: 20rpx;
}

.rightv{
  font-size:32rpx;
}
/*未选中*/
.coin {  
  height: 100rpx;
  display: flex;
  line-height: 100rpx;
  border-bottom: 1rpx solid #f2f2f2;
  justify-content: space-between;
  margin: 0 20rpx;
}