/**app.wxss**/

/*引入字体*/
@import '/styles/font.wxss';

/*背景样式*/
page {
  background-color: #F5F5F5;
  font-family: "Montserrat";
}

/*滚动条隐藏*/
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
  display: none;
}

/*字体部分*/
.font-t1{
  font-size: 82rpx;
}
.font-t2{
  font-size: 60rpx;
}
.font-t3{
  font-size: 48rpx;
}
.font-t4{
  font-size: 32rpx;
}
.font-t5{
  font-size: 28rpx;
}
.font-t6{
  font-size: 26rpx;
}
.font-t7{
  font-size: 24rpx;
}
.font-t8{
  font-size: 20rpx;
}

